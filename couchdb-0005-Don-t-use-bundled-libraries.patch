diff --git a/test/etap/test_util.erl.in b/test/etap/test_util.erl.in
index 352714e..cee79ec 100644
--- a/test/etap/test_util.erl.in
+++ b/test/etap/test_util.erl.in
@@ -27,11 +27,7 @@
