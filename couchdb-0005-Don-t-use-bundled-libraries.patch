-        "erlang-oauth",
-        "ibrowse",
-        "mochiweb",
-        "snappy"
+        "ejson"
     ],
     lists:foreach(fun(Name) ->
         code:add_patha(filename:join([builddir(), "src", Name]))
--- a/src/Makefile.am
+++ b/src/Makefile.am
@@ -18,11 +18,7 @@
     couch_replicator \
     couchdb \
     ejson \
-    erlang-oauth \
     etap \
